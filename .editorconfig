dotnet_naming_symbols.any_methods.applicable_kinds = method,delegate

dotnet_naming_symbols.visible_fields.applicable_kinds = field,property,event
dotnet_naming_symbols.visible_fields.applicable_accessibilities = public,protected

dotnet_naming_symbols.inner_fields.applicable_kinds = field,property,event
dotnet_naming_symbols.inner_fields.applicable_accessibilities = private,internal

dotnet_naming_symbols.any_entity.applicable_kinds = class,struct,enum
dotnet_naming_symbols.interface.applicable_kinds = interface
dotnet_naming_symbols.namespace.applicable_kinds = namespace
 
dotnet_naming_style.pascal_case_style.capitalization = pascal_case
dotnet_naming_style.camel_case_style.capitalization = camel_case
dotnet_naming_style.interface_style.capitalization = pascal_case
dotnet_naming_style.interface_style.required_prefix = I
dotnet_naming_style.inner_field_style.capitalization = camel_case
dotnet_naming_style.inner_field_style.required_prefix = _

dotnet_naming_rule.methods_rule.symbols  = any_methods
dotnet_naming_rule.methods_rule.style    = pascal_case_style
dotnet_naming_rule.methods_rule.severity = error

dotnet_naming_rule.visible_field_rule.symbols  = visible_fields
dotnet_naming_rule.visible_field_rule.style    = pascal_case_style
dotnet_naming_rule.visible_field_rule.severity = error

dotnet_naming_rule.inner_field_rule.symbols  = inner_fields
dotnet_naming_rule.inner_field_rule.style    = inner_field_style
dotnet_naming_rule.inner_field_rule.severity = error

dotnet_naming_rule.entities_rule.symbols  = any_entity
dotnet_naming_rule.entities_rule.style    = pascal_case_style
dotnet_naming_rule.entities_rule.severity = error

dotnet_naming_rule.interfaces_rule.symbols  = interface
